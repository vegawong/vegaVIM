{
  "diagnostic.errorSign": ">>",
  "diagnostic.infoSign": "--",
  "diagnostic.warningSign": ">>",
  "suggest.noselect": false,
  "tsserver.enableJavascript": false,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "explorer.toggle": true,
  "explorer.sources": [
    {
      "name": "buffer",
      "expand": false
    },
    {
      "name": "file",
      "expand": true
    }
  ],
  /* "tslint.autoFixOnSave": true, */
  "explorer.file.columns": [
    "git",
    "selection",
    "clip",
    "diagnosticError",
    "indent",
    "icon",
    "filename",
    ["fullpath"],
    ["size"],
    ["modified"],
    ["readonly"]
  ],
  "prettier.semi": false,
  "prettier.tabWidth": 2,
  "prettier.singleQuote": true,
}
