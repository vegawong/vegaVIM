{
  "snippets.extends": {
    "typescript": ["javascript"],
    "typescriptreact": ["javascript", "javascriptreact "]
  },
  "diagnostic.errorSign": ">>",
  "diagnostic.infoSign": "--",
  "diagnostic.warningSign": ">>",
  "suggest.noselect": false,
  "tsserver.enableJavascript": false,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "explorer.toggle": true,
  "explorer.sources": [
    {
      "name": "buffer",
      "expand": true
    },
    {
      "name": "file",
      "expand": true
    }
  ],
  "tslint.autoFixOnSave": true,
  "explorer.file.columns": [
    "git",
    "selection",
    "clip",
    "diagnosticError",
    "indent",
    "icon",
    "filename",
    ["fullpath"],
    ["size"],
    ["modified"],
    ["readonly"]
  ]
}
